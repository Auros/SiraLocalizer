{
  "profiles": {
    "Beat Saber": {
      "commandName": "Executable",
      "executablePath": "$(BeatSaberDir)\\Beat Saber.exe",
      "commandLineArgs": "--verbose --debug --trace",
      "workingDirectory": "$(BeatSaberDir)",
      "environmentVariables": {
        "SteamAppId": "620980"
      }
    },
    "Beat Saber (FPFC)": {
      "commandName": "Executable",
      "executablePath": "$(BeatSaberDir)\\Beat Saber.exe",
      "commandLineArgs": "fpfc --verbose --debug --trace",
      "workingDirectory": "$(BeatSaberDir)",
      "environmentVariables": {
        "SteamAppId": "620980"
      }
    },
    "Beat Saber (dump localization)": {
      "commandName": "Executable",
      "executablePath": "$(BeatSaberDir)\\Beat Saber.exe",
      "commandLineArgs": "fpfc --verbose --trace --dump-localization",
      "workingDirectory": "$(BeatSaberDir)",
      "environmentVariables": {
        "SteamAppId": "620980"
      }
    }
  }
}